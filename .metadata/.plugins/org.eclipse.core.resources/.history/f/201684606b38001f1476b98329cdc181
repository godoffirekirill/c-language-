#include <stdio.h>

void fan(int mode);

int main() {
	 fan(3);



	return 0;
}

void fan(int mode){

	if (mode < 1 || mode > 4) {
			printf(" Wrong mode.\n");
			return;
		}
	if (mode == 1) {
		printf(" Off\n");
			return;
	}
	if (mode == 2) {
		printf(" Low\n");
		return;
	}
	if (mode == 3) {
		printf(" Middle\n");
		return;
	}
	if (mode == 4) {
		printf(" Hi\n");
		return;
		}

}
